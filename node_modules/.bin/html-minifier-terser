#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules:/Users/berat/Downloads/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/html-minifier-terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../html-minifier-terser/cli.js" "$@"
fi
