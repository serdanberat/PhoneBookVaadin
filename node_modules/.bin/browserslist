#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/browserslist@3.2.8/node_modules/browserslist/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/browserslist@3.2.8/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules:/Users/berat/Downloads/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/browserslist/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browserslist/cli.js" "$@"
else
  exec node  "$basedir/../browserslist/cli.js" "$@"
fi
