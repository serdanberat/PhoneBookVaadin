#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/webpack-dev-server@3.11.0_68d0005888c13e79f5e307f4e918d09a/node_modules/webpack-dev-server/bin/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/webpack-dev-server@3.11.0_68d0005888c13e79f5e307f4e918d09a/node_modules/webpack-dev-server/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/webpack-dev-server@3.11.0_68d0005888c13e79f5e307f4e918d09a/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/.pnpm/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules:/Users/berat/Downloads/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/webpack-dev-server/bin/node_modules:/Users/berat/Downloads/VaadinMySQL/node_modules/webpack-dev-server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
